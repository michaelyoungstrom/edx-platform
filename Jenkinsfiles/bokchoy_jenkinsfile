pipeline {
    agent { label 'master' }
    stages {
        stage('Parallel Stage') {
            parallel {
                stage("one") {
                    agent {
                        label 'master'
                    }
                    steps {
                        echo "On Branch A"
                    }
                }
                stage("two") {
                    agent {
                        label 'master'
                    }
                    steps {
                        echo "On Branch B"
                    }
                }
            }
        }
    }
}